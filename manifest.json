{
  "manifest_version": 2,
  "name": "Eyebrowse",
  "version": "1.0",
  "description": "Eyebrowse by CSAIL",
  "browser_action": {
    "default_icon": "img/eye.png",
    "default_popup":"popup.html"
  },
  "background": {
      "scripts": ["js/background.js"]
    },
    "permissions": [
      "notifications",
      "tabs"
    ]
}
